{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Database": "Error"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "Database": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "None"
      },
      "TableName": "tb_masterdata_log"
    },
    "File": {
      "LogLevel": {
        "Default": "Error",
        "Microsoft.AspNetCore": "None"
      },
      "FileName": "Log/AppLog-yyyyMMdd.txt"
    }
  },
  "JJMasterData": {
    "DataDictionaryTableName": "tb_masterdata",
    "ConnectionString": "Server=localhost;Database=JJMasterData;Integrated Security=True;Trust Server Certificate=true",
    "ResourcesTableName": "tb_masterdata_resources",
    "ReadProcedurePattern": "jj_get{tablename}",
    "WriteProcedurePattern": "jj_set{tablename}",
    "BootstrapVersion": 3,
    "SecretKey": "ExampleSecretKey",
    "Theme": "dark-blue"
  },
  "AllowedHosts": "*"
}
