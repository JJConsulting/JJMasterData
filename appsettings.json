{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ConnectionString": "data source=localhost,1433;initial catalog=JJMasterData;user=sa;password=Test@123456"
  },
  "ConnectionProviders": {
    "ConnectionString": "System.Data.SqlClient"
  },
  "JJMasterData": {
    "MongoDB": {
      "ConnectionString": "mongodb://localhost:27017",
      "DatabaseName": "jjmasterdata",
      "CollectionName": "metadata"
    },
    "TableName": "tb_masterdata",
    "ResourcesTableName": "tb_masterdata_resources",
    "PrefixGetProc": "sp_{tablename}_get",
    "PrefixSetProc": "sp_{tablename}_set",
    "LayoutPath": "_Layout",
    "PopUpLayoutPath": "_Layout.PopUp",
    "BootstrapVersion": 5,
    "Theme": "dark-blue",
    "Logger": {
      "Table": {
        "Name": "tb_masterdata_log"
      },
      "FileName": "../../Log/yyyyMMdd_applog.txt",
      "WriteInFile": "All",
      "WriteInDatabase": "All"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
