{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ConnectionString": "data source=localhost,1433;initial catalog=JJMasterData;user=sa;password=Test@123456;"
  },
  "ConnectionProviders": {
    "ConnectionString": "System.Data.SqlClient"
  },    
  "JJMasterData": {
    "TableName": "tb_masterdata",
    "ResourcesTableName": "tb_masterdata_resources",
    "PrefixGetProc": "jj_get{tablename}",
    "PrefixSetProc": "jj_set{tablename}",
    "BootstrapVersion": 5,
    "Theme": "dark-blue",
    "Logger": {
      "Table": {
        "Name": "tb_masterdata_log"
      },
      "FileName": "../../Log/yyyyMMdd_applog.txt",
      "WriteInFile": "All",
      "WriteInDatabase": "All"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
