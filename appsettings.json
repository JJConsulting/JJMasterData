{
  "JJMasterData": {
    "DataDictionaryTableName": "MasterData",
    "ConnectionString": "Server=localhost;Database=JJMasterData;Integrated Security=True;Trust Server Certificate=true",
    "ReadProcedurePattern": "{tablename}Get",
    "WriteProcedurePattern": "{tablename}Set",
    "SecretKey": "ExampleSecretKey"
  },
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Database": "Error"
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "Database": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "None"
      },
      "TableName": "tb_masterdata_log"
    },
    "File": {
      "LogLevel": {
        "Default": "Error",
        "Microsoft.AspNetCore": "None"
      },
      "FileName": "Log/AppLog-yyyyMMdd.txt"
    }
  }
}
