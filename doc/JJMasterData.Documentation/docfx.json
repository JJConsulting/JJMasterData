{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../../src",
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**",
            "_plugins/**",
            "**/Plugins/**.cs*",
            "**/example/**",
            "**/test/**",
            "**/JJMasterData.WebApi/**",
            "**/test/**",
            "**/JJMasterData.SourceControl/**"
          ]
        }
      ],
      "dest": "lib",
      "properties": {
        "TargetFramework": "net6.0"
      },
      "namespaceLayout":"nested",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "lib/**.yml",
          "lib/**.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "index.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "example/JJMasterData.WebExample/**",
          "media/**",
          "logo.png",
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "lib/**.md"
        ],
        "exclude": [
          "obj/**",
          "plugins/**",
          "templates/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appName":"JJMasterData",
      "_disableContribution": false,
      "_enableSearch": "true",
      "_gitContribute": {
        "branch": "main",
        "repo": "https://github.com/jjconsulting/JJMasterData",
        "apiSpecFolder": "../../src"
      },
      "_gitUrlPattern": "github",
      "_disableSideFilter":true,
      "_appTitle": "JJMasterData",
      "_appLogoPath": "logo.png",
      "_appFooter": "Saturn Version - 3.1",
      "_copyrightFooter":"2023 JJConsulting. All rights reserved."
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/singulinkfx"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "noLangKeyword": false,
    "keepFileLink": true,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
