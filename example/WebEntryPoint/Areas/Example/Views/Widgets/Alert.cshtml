@{
    ViewData["Title"] = "JJAlert";
}

<jj-title title="JJAlert" subtitle="A wrapper for Bootstrap alerts."/>

<alert
    title="This is a Success JJAlert."
    message="You can use any variation."
    color="Success"/>
    
