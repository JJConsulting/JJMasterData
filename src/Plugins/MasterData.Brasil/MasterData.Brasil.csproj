        <LangVersion>12</LangVersion>
        <Nullable>enable</Nullable>
        <RootNamespace>JJMasterData.Brasil</RootNamespace>
        <AssemblyName>JJMasterData.Brasil</AssemblyName>
        <Version>1.0.0</Version>
