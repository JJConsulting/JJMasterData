<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
		<LangVersion>12</LangVersion>
        <Nullable>enable</Nullable>
        <AssemblyName>JJMasterData.NCalc</AssemblyName>
        <RootNamespace>JJMasterData.NCalc</RootNamespace>
        <Description>JJMasterData NCalc expressions support.</Description>
        <PackageIcon>JJMasterData.png</PackageIcon>
        <PackageId>JJMasterData.NCalc</PackageId>
        <PackageTags>ncalc,jjmasterdata</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageReleaseNotes>Update NCalc to 4.0.</PackageReleaseNotes>
   	    <Version>2.0.0</Version>
    	<AssemblyVersion>2.0.0</AssemblyVersion>
        <FileVersion>2.0.0</FileVersion>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\..\Core\Core.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="NCalc.DependencyInjection" Version="4.2.0" />
      <PackageReference Include="NCalc.MemoryCache" Version="4.2.0" />
      <PackageReference Include="NCalcSync" Version="4.2.0" />
    </ItemGroup>


</Project>
