@model string
@inject IOptionsSnapshot<MasterDataWebOptions> Options
@inject IStringLocalizer<MasterDataResources> StringLocalizer
@{
    ViewData["Title"] = StringLocalizer["Localization"];
    Layout = Options.Value.ModalLayoutPath;
}

<master-data-form asp-action="Index" asp-controller="Localization" asp-area="DataDictionary">
    @Html.Raw(Model)
</master-data-form>
