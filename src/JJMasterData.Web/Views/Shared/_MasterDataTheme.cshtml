@using JJMasterData.Web.Services
@inject ThemeService ThemeService

<script type="text/javascript" src="~/js/dark-reader/dark-reader.min.js"></script>
<script>
    theme = @((int)ThemeService.GetTheme());

    if (theme == 1) {
        DarkReader.enable({
            brightness: 100,
            contrast: 90,
            sepia: 10
        })
    }
    else {
        DarkReader.disable()
    }
</script>