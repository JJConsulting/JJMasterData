<script src="~/js/jquery/jquery.js"></script>
<script src="~/js/jquery/jquery-form/jquery.form.js"></script>
<script src="~/js/jquery/jquery-number/jquery.number.min.js"></script>
<script src="~/js/jquery/jquery-ui/jquery-ui.min.js"></script>
<script src="~/js/jquery/jquery-validation/jquery.validate.min.js"></script>
<script src="~/js/jquery/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

@switch (BootstrapHelper.Version)
{
    case 3:
        <script type="text/javascript" src="~/js/bootstrap/bootstrap3/bootstrap.min.js"></script>
        <script type="text/javascript" src="~/js/bootstrap/bootstrap3-toggle/bootstrap-toggle.min.js"></script>
        <script type="text/javascript" src="~/js/bootstrap/bootstrap-select/bootstrap-select.min.js"></script>
        break;
    case 4:
        <script type="text/javascript" src="~/js/bootstrap/bootstrap4/bootstrap.bundle.min.js"></script>
        <script type="text/javascript" src="~/js/bootstrap/bootstrap4-toggle/bootstrap4-toggle.min.js"></script>
        <script type="text/javascript" src="~/js/bootstrap/bootstrap-select/bootstrap-select.min.js"></script>
        break;
    default:
        <script type="text/javascript" src="~/js/bootstrap/bootstrap5/bootstrap.bundle.min.js"></script>
        <script type="text/javascript" src="~/js/bootstrap/bootstrap4-toggle/bootstrap4-toggle.min.js"></script>
        <script type="text/javascript" src="~/js/bootstrap/bootstrap-select/bootstrap-select-bs5.min.js"></script>
        break;
}

<script type="text/javascript" src="~/js/flatpickr/flatpickr.min.js"></script>
<script type="text/javascript" src="~/js/flatpickr/l10n/pt.min.js"></script>
<script type="text/javascript" src="~/js/flatpickr/l10n/en.min.js"></script>
<script type="text/javascript" src="~/js/popperjs/popper.min.js"></script>
<script type="text/javascript" src="~/js/jquery/jquery-uploadfile/jquery.uploadfile.min.js"></script>
<script type="text/javascript" src="~/js/spin/spin.min.js"></script>
<script type="text/javascript" src="~/js/inputmask/dist/inputmask.min.js"></script>
<script type="text/javascript" src="~/js/inputmask/dist/bindings/inputmask.binding.js"></script>
<script type="text/javascript" src="~/js/bootstrap/bootstrap-typeahead/bootstrap-typeahead.min.js"></script>
<script type="text/javascript" src="~/js/bootstrap/bootstrap-tagsinput/bootstrap-tagsinput.min.js"></script>

<environment include="Development">
    <script asp-append-version="true" type="text/javascript" src="~/js/jjmasterdata/jjmasterdata.js"></script>
</environment>
<environment exclude="Development">
    <script asp-append-version="true" type="text/javascript" src="~/js/jjmasterdata/jjmasterdata.min.js"></script>
</environment>

